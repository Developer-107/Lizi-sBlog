<header class="masthead pt-5 mt-4">
            <div class="container position-relative px-4 px-lg-5">
                <div class="row gx-4 gx-lg-5 justify-content-center">
                    <div class="col-md-10 col-lg-8 col-xl-7">
                        <div class="site-heading pt-5">
                            <h1>Contact me</h1>
                        </div>
                    </div>
                </div>
            </div>
</header>
</section>


<section class="py-5 my-5 px-5 mx-5 ">
    <div class="container px-4 px-lg-5 pt-5 px-5 py-5 my-5">
            <div class="gx-4 gx-lg-5  justify-content-center">
                <div class="row wrap-contact100 justify-content-center">
				<div class="col-4 pt-5 mt-5">
					<img src="/images/mail_275x202.jpeg" alt="IMG">
				</div>

				<form action="mailto:<%=email%>" method="post" enctype="text/plain" class="col-4 validate-form text-center">
					<h3>
						Get in touch
					</h3>

					<div class=" py-3 validate-input" data-validate="Name is required">
						<input class="input100 " type="text" name="name" placeholder="Name" required>
					</div>

					<div class="wrap-input100 validate-input" data-validate="Valid email is required: ex@abc.xyz">
						<input class="input100" type="text" name="email" placeholder="Email">
					</div>

					<div class="py-3 wrap-input100 validate-input" data-validate="Message is required">
						<textarea class="input100" name="message" placeholder="Message"></textarea>
					</div>

					
						<button id="sendButton" class="btn btn-primary text-uppercase">
							Send
						</button>
					
				</form>
			</div>
            </div>
        </div>

</section>
<br>
